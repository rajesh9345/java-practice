import java.util.*;
import java.util.ArrayList;
public class Main
{
    int second(int[] arr)
    {
        Arrays.sort(arr);
        return arr[1];
    }
	public static void main(String[] args) {
	    Scanner s=new Scanner(System.in);
		System.out.println("enter size of array");
		int n=s.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<arr.length;i++)
        {
            arr[i]=s.nextInt();
        }
       
		Main m=new Main();
		m.second(arr);
		System.out.println("second smallest element "+m.second(arr));
		
	}
}
